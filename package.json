{
  "name": "lab4",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "license": "ISC",
  "author": "",
  "scripts": {
    "client:build": "cd client && npm run build",
    "client:install": "cd client && npm install",
    "client:lint": "cd client && npm run lint",
    "client:lint:fix": "cd client && npm run lint:fix",
    "client:start": "cd client && npm run start",
    "install": "npm run client:install && npm run server:install",
    "lint": "npm run client:lint && npm run server:lint",
    "lint:fix": "npm run client:lint:fix && npm run server:lint:fix",
    "server:install": "cd server && npm install",
    "server:lint": "cd server && npm run lint",
    "server:lint:fix": "cd server && npm run lint:fix",
    "server:start": "cd server && npm run start",
    "server:dev": "cd server && npm run dev",
    "start": "npm run client:build && npm run server:start",
    "dev": "concurrently \"npm run server:dev\" \"npm run client:start\""
  },
  "dependencies": {
    "concurrently": "^7.2.2"
  }
}
